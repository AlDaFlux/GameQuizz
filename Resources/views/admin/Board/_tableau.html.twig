     <table class='table table-bordered'>
                <thead>
                    <tr>
                        <th>Ordre</th>
                        <th>Nom</th>
                        <th>Question</th>
                        <th>NbReponses</th>
                        <th>Publi√©e</th>
                        <th>Question au</th>
                    </tr>
                </thead>
                <tbody>
        {%  for  question in board.questions %}
                    <tr>
                        <td>{{ question.ordre}}</td>
                        <td><a href='{{ path("algq_admin_question_show", {'id':question.id}) }}'>{{ question}}</a></td>
                        <td>{{ question.questionText}}</td>
                        <td>{{ question.answersCount}}</td>
                        <td>{{ question.published}}</td>
                            {%  if  question.questionAudio is not empty %}
                                <td>
                                    <audio width='30px' controls title='{{ question.questionAudio}}'><source src="/{{ question.questionAudio}}" ></audio> 
                                </td>
                            {%  else %}
                                <td class='alert-danger'></td>
                            {%  endif  %}
                            {%  if  question.answerAudio is not empty %}
                                <td><audio width='30px' controls title='{{ question.answerAudio}}'><source src="/{{ question.answerAudio}}" ></audio> </td>
                            {%  else   %}
                                <td class='alert-danger'></td>
                            {%  endif  %}
                    </tr>
        {%  endfor  %}
                </tbody>
    </table>
                